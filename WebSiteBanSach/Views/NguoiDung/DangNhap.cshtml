
@{
    ViewBag.Title = "DangNhap";
    Layout = "~/Views/Layout/NguoiDungLayout.cshtml";
}
<div class="TaiKhoan">
    <h2>DangNhap</h2>
    <h1>@ViewBag.ThongBao</h1>
    @using (Html.BeginForm())
    {

        <span>Tài khoản: </span><br />
        @Html.TextBox("txtTaiKhoan");
        <br />
        <span>Mật khẩu: </span><br />
        <input name="txtMatKhau" type="text" />
        <br />
        <input type="submit" id="btnSubmit" name="btnSubmit" />

    }
</div>
<br />
<style>
    .TaiKhoan {
        margin-left:185px;
    }
</style>